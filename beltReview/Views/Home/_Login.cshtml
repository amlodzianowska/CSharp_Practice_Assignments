@model LogUser;
<h1>Login</h1>
        <form action="login" method="post">
            <div class="form-group">
                <label for="LEmail">Email</label>
                <input asp-for="LEmail" class="form-control" value="s@seagull.com">
                <span asp-validation-for="LEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label for="LPassword">Password</label>
                <input asp-for="LPassword" class="form-control" value="password">
                <span asp-validation-for="LPassword" class="text-danger"></span>
            </div>
            <input type="submit" value="Login" class="btn btn-warning">
        </form>